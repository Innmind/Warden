{
    "name": "innmind/warden",
    "type": "library",
    "description": "Tool to restrict ssh connections",
    "keywords": [],
    "homepage": "http://github.com/Innmind/Warden",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Warden/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "~3.5",
        "innmind/genome": "^3.0",
        "innmind/cli-framework": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Warden\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Warden\\": "tests/",
            "Fixture\\Innmind\\Warden\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/black-box": "^4.9",
        "innmind/coding-standard": "^1.1"
    },
    "bin": ["warden"],
    "provide": {
        "innmind/genome-genes": "3.0"
    }
}
